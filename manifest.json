{
  "name": "nosubvod-twitch",
  "version": "0.1.0",
  "description": "Show sub-only VOD on Twitch",
  "manifest_version": 3,
  "icons": {
    "128": "assets/icons/icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "dist/app.js",
        "dist/page_init.js",
        "dist/inject.js",
        "dist/patch_amazonworker.js"
      ],
      "matches": [
        "https://*.twitch.tv/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": ["*://*.twitch.tv/*"],
      "js": [
  "dist/restriction-remover.js",
  "dist/nosubvod.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "host_permissions": [
    "https://*.twitch.tv/*",
    "https://static.twitchcdn.net/assets/*"
  ],
  "permissions": []
}